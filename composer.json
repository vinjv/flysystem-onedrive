{
    "name": "hevelius/flysystem-onedrive",
    "description": "Flysystem Adapter for the OneDrive API",
    "keywords": [
        "flysystem-onedrive",
        "flysystem",
        "onedrive",
        "api"
    ],
    "homepage": "https://github.com/hevelius/flysystem-onedrive",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Perrotta",
            "email": "mario.perrotta@unimi.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "league/flysystem": "^3.2",
        "microsoft/microsoft-graph": "^1.74",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "laravel/framework": "^9.19",
        "orchestra/testbench": "^v7.1.0"
    }
    "autoload": {
        "psr-4": {
            "MarioPerrotta\\FlysystemOneDrive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarioPerrotta\\FlysystemOneDrive\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
